<link href='/css/error.css' rel="stylesheet">
<%if(errors!=='null'&&errors.length!=0){%>
    <div class="error" id="error">
        <%errors.forEach((errField)=>{%>
            <p>
                <%=String(errField.param[0]).toUpperCase()+String(errField.param).slice(1)%> :: <%=errField.msg%>
            </p>
            <% })%>

    </div>
    <%}%>